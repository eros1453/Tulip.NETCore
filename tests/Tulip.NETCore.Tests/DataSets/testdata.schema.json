{
  "$id": "https://github.com/hmG3/Tulip.NETCore/testdata.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON Schema for data used in unit tests",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Unique name of an indicator."
          },
          "inputs": {
            "type": "array",
            "description": "An array of input values.",
            "minItems": 1,
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "options": {
            "type": "array",
            "description": "An array of options.",
            "items": {
              "type": "number"
            }
          },
          "outputs": {
            "type": "array",
            "description": "An array of output value.",
            "minItems": 1,
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "skip": {
            "type": "boolean",
            "description": "Flag indicates to skip test."
          }
        },
        "additionalProperties": false
      }
    }
  }
}
